    "version": "0.2.0",
    "configurations": [

        {
            "name": "Server",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver",
                "0.0.0.0:9999"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/server/manage.py",
            "python": "${workspaceFolder}/server/venv/bin/python",
            "cwd": "${workspaceFolder}/server",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/server"
            },
            "console": "integratedTerminal"
        }
    ]
}